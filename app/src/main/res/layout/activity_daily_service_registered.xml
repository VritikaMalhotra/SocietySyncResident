<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Features.DailyServiceRegistered">

    <TextView
        android:id="@+id/textViewDailyServiceRegistered"
        android:layout_width="153dp"
        android:layout_height="93dp"
        android:layout_marginStart="127dp"
        android:layout_marginTop="160dp"
        android:layout_marginEnd="131dp"
        android:layout_marginBottom="118dp"
        android:text="Daily service already registered your flat and block number has been added to the list"
        app:layout_constraintBottom_toTopOf="@+id/buttonDailyServiceRegisteredContinue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonDailyServiceRegisteredContinue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="161dp"
        android:layout_marginTop="118dp"
        android:layout_marginEnd="159dp"
        android:layout_marginBottom="312dp"
        android:text="Continue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewDailyServiceRegistered" />
</androidx.constraintlayout.widget.ConstraintLayout>